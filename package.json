{
   "name": "spm-server",
   "version": "0.0.1",
   "description": "Sell Product Manager",
   "main": "dist/index.js",
   "scripts": {
      "dev": "set NODE_ENV=dev&&nodemon src/index.ts",
      "create_migration_dev": "ts-node ./node_modules/typeorm/cli.js migration:generate -n seed",
      "migration_dev": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "migration_prod": "typeorm migration:run",
      "build": "tsc -p . && npm run migration_prod",
      "start": "node ."
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/PhamDuong98/spm-server.git"
   },
   "author": "PhamDuong98",
   "license": "MIT",
   "dependencies": {
      "body-parser": "^1.18.1",
      "cors": "^2.8.5",
      "express": "^4.15.4",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.24"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.0",
      "@types/cors": "^2.8.6",
      "@types/express": "^4.17.6",
      "@types/node": "^8.0.29",
      "nodemon": "^2.0.3",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   }
}
